{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint algolia-indexer.js",
    "start": "hugo -d ../dist -s site -vw",
    "preview": "hugo -d ../dist -s site -D -F",
    "prebuild": "rimraf dist",
    "build": "npm run build:hugo",
    "build:preview": "npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "postbuild": "node algolia-indexer.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "postcss-cli": "^7.1.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.3.0",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "algoliasearch": "^4.2.0",
    "jsdom": "^16.4.0"
  }
}
